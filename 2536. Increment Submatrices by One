//mainaging brute force and preventing tle

var rangeAddQueries = function (n, queries) {
    // Step 1: Create difference matrix (n+1 x n+1)
    let diff = Array.from({ length: n + 1 }, () =>
        new Array(n + 1).fill(0)
    );

    // Step 2: Apply each query using corner marking
    for (let [r1, c1, r2, c2] of queries) {
        diff[r1][c1] += 1;
        diff[r1][c2 + 1] -= 1;
        diff[r2 + 1][c1] -= 1;
        diff[r2 + 1][c2 + 1] += 1;
    }

    // Step 3: Prefix sum row-wise
    for (let i = 0; i < n; i++) {
        for (let j = 1; j < n; j++) {
            diff[i][j] += diff[i][j - 1];
        }
    }

    // Step 4: Prefix sum column-wise
    for (let j = 0; j < n; j++) {
        for (let i = 1; i < n; i++) {
            diff[i][j] += diff[i - 1][j];
        }
    }

    // Step 5: Extract final n x n matrix
    let result = Array.from({ length: n }, () => new Array(n));
    for (let i = 0; i < n; i++) {
        for (let j = 0; j < n; j++) {
            result[i][j] = diff[i][j];
        }
    }

    return result;
};
